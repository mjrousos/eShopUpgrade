@model IEnumerable<string>

<p>
    @Html.ActionLink("Upload New", "Upload")
</p>
<table class="table">
    <tr>
        <th>
            Document
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item, "Download", new { filename = item })
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { filename = item })
        </td>
    </tr>
}

</table>
